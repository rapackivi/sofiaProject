<%- include("partials/header") -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group" >
        <label>title</label>
        <input class="form-control" type="text" name="titlePost" required="true" minlength="1"  >
    </div>
    <div class="form-group" >
        <label>post</label>
        <textarea class="form-control" name="bodyPost" cols="30" rows="10" required="true" minlength="8" ></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>

<%- include("partials/footer") -%>